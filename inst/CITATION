citHeader("To cite RTDE in publications use:")


citEntry(entry="Manual",
         title = "Tsallis distribution",
         author = "Cosma Shalizi, Christophe Dutang",
	 year = 2015,
         note = "R package version 0.9-1",

         textVersion =
	 paste(	"C. Shalizi, C. Dutang",
		"(2015),",
	       	"Tsallis distribution")
)

